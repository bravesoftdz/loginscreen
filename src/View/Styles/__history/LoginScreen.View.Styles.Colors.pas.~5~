unit LoginScreen.View.Styles.Colors;

interface
  type
    TStyles = class
    H1, H2, H3, H4 : integer;
    PANEL_COLOR : integer;
    BUTTON_COLOR : integer;
    public

    constructor Create;
    private
    end;


implementation

{ TColor }

constructor TStyles.Create;
begin
  PANEL_COLOR := $00DA8E49;
  BUTTON_COLOR := $00DA8E49;

  H1 := 24;
  H2 := 18;
  H3 := 14;
  H4 := 12;
  inherited
end;

end.
